import React ,{useState} from 'react'
import './Keies.css'

function Keies(props){

    const handleClick = () =>{
      props.displayFunction(props.name)
    }

    return(
        <div>
            <button onClick={handleClick} id={props.id}>{props.name}</button>
        </div>
    )
}




export default Keies;